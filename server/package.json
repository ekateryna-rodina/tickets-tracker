  "dependencies": {
    "@databases/pg": "^5.1.0",
    "@databases/pg-test": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "pg": "^8.6.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.11",
    "dotenv": "^10.0.0",
    "pg-mem": "^1.9.9"
  }
